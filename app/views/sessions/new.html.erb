<h1>登入頁面</h1>

<%= form_for(@user, url: sign_in_sessions_path) do |form|%>

  <div class="field">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>

  <div class="field">
    <%= form.label :password %>
    <%= form.password_field :password %>
  </div>
  
  <%= form.submit '登入'%>

<% end %>